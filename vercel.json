{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://renovation-task-manager-d2evcgk82vjt19ur26rg.lp.dev/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
